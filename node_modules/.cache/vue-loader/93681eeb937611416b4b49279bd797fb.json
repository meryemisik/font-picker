ass\"\n        :size=\"iconSize\"\n        class=\"button__icon\"\n      />\n\n      <span v-if=\"!circle\" :class=\"{ 'button-content--loading': loading }\">\n        <slot name=\"default\" />\n      </span>\n\n      <span v-if=\"loading\" class=\"button__loading-icon\">\n        <svg viewBox=\"0 0 24 24\">\n          <g>\n            <path\n              fill=\"currentColor\"\n              fill-opacity=\".9\"\n              fill-rule=\"evenodd\"\n              d=\"M3.267 8.157a12.307 12.307 0 013-3.389c1.73-1.35 3.759-2.196 5.732-2.197a9.466 9.466 0 018.86 6.21.857.857 0 001.61-.59A11.18 11.18 0 0012.001.857H12c-2.451 0-4.842 1.04-6.788 2.56A14.109 14.109 0 001.983 6.97L1.69 5.792a.857.857 0 10-1.663.416L.88 9.629a.857.857 0 001.054.628l3.416-.854a.857.857 0 10-.416-1.663l-1.668.417zm19.855 6.225a.857.857 0 00-1.055-.64l-3.418.855a.857.857 0 00.416 1.663l1.726-.431a10.745 10.745 0 01-8.814 5.6 9.467 9.467 0 01-8.836-6.21.857.857 0 10-1.61.59 11.18 11.18 0 0010.467 7.334l.002-.857.048.855a12.458 12.458 0 009.988-6.035l.275 1.102a.857.857 0 101.663-.416l-.852-3.41z\"\n              clip-rule=\"evenodd\"\n            />\n          </g>\n        </svg>\n      </span>\n    </button>\n  </div>\n</template>\n\n<script>\nimport \"../../../assets/scss/button.scss\";\nimport fpIcon from \"../Icon/index.vue\";\nexport default {\n  name: \"fpButton\",\n  components: {\n    fpIcon,\n  },\n  props: {\n    fluid: {\n      type: Boolean,\n      default: false,\n    },\n\n    size: {\n      type: String,\n      default: \"medium\",\n      validator(value) {\n        return [\"tiny\", \"small\", \"medium\", \"large\"].indexOf(value) !== -1;\n      },\n    },\n\n    styles: {\n      type: String,\n      default: \"primary\",\n      validator(value) {\n        return (\n          [\n            \"primary\",\n            \"secondary\",\n            \"tertiary\",\n            \"success\",\n            \"danger\",\n            \"warning\",\n            \"formpro\",\n            \"formpro--outline\",\n            \"outline\",\n          ].indexOf(value) !== -1\n        );\n      },\n    },\n\n    disabled: {\n      type: Boolean,\n      default: false,\n    },\n\n    circle: {\n      type: Boolean,\n      default: false,\n    },\n\n    icon: {\n      type: String,\n      default: null,\n    },\n\n    iconPosition: {\n      type: String,\n      default: \"left\",\n      validator(value) {\n        return [\"left\", \"right\"].indexOf(value) !== -1;\n      },\n    },\n\n    loading: {\n      type: Boolean,\n      default: false,\n    },\n\n    // dropdownMenu: {\n    //   type: String,\n    // },\n    type: {\n      type: String,\n      default: \"button\",\n      validator(value) {\n        return [\"button\", \"link\"].indexOf(value) !== -1;\n      },\n    },\n  },\n  computed: {\n    classes() {\n      return {\n        [`${this.type === \"link\" ? \"button-link\" : \"button\"}--${\n          this.styles\n        }`]: true,\n        [`button--${this.size}`]: true,\n        \"button--fluid\": this.fluid,\n        \"button--circle\": this.circle,\n        loading: this.loading,\n      };\n    },\n    classesType() {\n      return {\n        [`${this.type === \"link\" ? \"button-link\" : \"button\"}`]: true,\n      };\n    },\n    defaultIcon() {\n      if (!this.icon && this.circle) {\n        return \"add\";\n      }\n      return this.icon;\n    },\n    iconSize() {\n      switch (this.size) {\n        case \"tiny\":\n          return 12;\n\n        case \"large\":\n          return 20;\n\n        default:\n          return 16;\n      }\n    },\n    isButtonDisable() {\n      return this.disabled || this.loading;\n    },\n    iconClass() {\n      return {\n        [`is-icon-${this.iconPosition}`]: true,\n      };\n    },\n  },\n};\n</script>\n"]}]}